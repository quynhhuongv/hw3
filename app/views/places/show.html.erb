<h1><%= @place["name"] %></h1>
<br>

  <% for post in @posts %>
    <h2><%= post["title"] %></h2>
    <p><%= post["posted_on"] %></p>
    <h3><%= post["description"] %></h3>
  <% end %>

</br>
<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>

<p><a href="/places">Back to All Places</a></p>
  
